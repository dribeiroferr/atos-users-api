  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-typescript": "^7.21.5",
    "@types/bcrypt": "^5.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.59.8",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "typeorm": "^0.3.17",
    "typescript": "*"
  },
  "scripts": {
    "build": "rm -rf ./dist/ ** tsc -p .",
    "build:deploy": "yarn build && yarn test",
    "dev": "nodemon",
    "dev:set": "cross-env NODE_ENV=dev",
    "lint:OS:fix": "sed -i -e 's/\r$//' ./jest.config.ts",
    "start:dev": "ts-node-dev --transpile-only --ignore-watch node_modules ./src/infra/servers/index.ts",
    "start": "node ./dist/infra/server/index.js",
    "test": "jest",
    "win:migrations:create": "migrations-create",
    "unix:migrations:set": "sudo chmod +x migrations-create.sh",
    "unix:migrations:create": "migrationName=$npm_config_name ./migrations-create.sh $migrationName",
    "migrations:run": "typeorm migration:run --config ./src/utils/ormconfig.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
